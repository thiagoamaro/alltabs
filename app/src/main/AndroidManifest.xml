<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.prototipoat">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/at_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AllTabsTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".ComandaActivity" />
        <activity android:name=".CardapioLanche" />
        <activity android:name=".RestauranteActivity"/>
        <activity android:name=".Pagamento" android:windowSoftInputMode="adjustResize"/>
    </application>

</manifest>